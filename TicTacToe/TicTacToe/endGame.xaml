<Window x:Class="TicTacToe.endGame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicTacToe"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="endGame" Height="350" Width="350">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40*"/>
            <RowDefinition Height="150*"/>
            <RowDefinition Height="115*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <TextBlock Grid.Row="1" Grid.ColumnSpan="2" Width="250" Height="100" FontSize="25" Name="winner" TextAlignment="Center"/>
        <Button Grid.Row="2" Grid.Column="0" Width="100" Height="50" Content="Play again" FontSize="20" Click="Button_Click"/>
        <Button Grid.Row="2" Grid.Column="1" Width="100" Height="50" Content="End game" FontSize="20" Click="Button_Click_1"/>
        
    </Grid>
</Window>
